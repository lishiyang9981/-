#include<stdio.h>
//指针数组
int main()
{
	int a1[] = { 1,2,3,4,5 };
	int a2[] = { 2,3,4,5,6 };
	int a3[] = { 3,4,5,6,7 };
	int a4[] = { 4,5,6,7,8 };
	int* pa[] = { a1,a2,a3,a4 };
	int i = 0;
	for (i = 0; i <4; i++)
	{
		int j = 0;
		for (j = 0; j < 5; j++)
		{
			printf("%d", *(pa[i]+j));
		}
		printf("\n");
	}
	return 0;
}
